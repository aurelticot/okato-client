{
  "name": "okato-client",
  "version": "0.1.0",
  "description": "Stock Echange trading sessions - Client side of the okato project",
  "private": true,
  "scripts": {
    "clean:node_modules": "rm -rf node_modules/",
    "clean:build": "rm -rf build/",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "format:check": "npx prettier src/ --check",
    "format": "npx prettier src/ --write",
    "lint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint": "eslint src/ --fix --ext .js,.jsx,.ts,.tsx",
    "check": "npm run format:check && npm run lint:check",
    "fix": "npm run format && npm run lint",
    "prebuild": "npm run clean:build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start:dev": "react-scripts start",
    "start": "npm run start:dev",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aurelticot/okato-client.git"
  },
  "keywords": [
    "stock-market"
  ],
  "author": "aurelticot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aurelticot/okato-client/issues"
  },
  "homepage": "https://github.com/aurelticot/okato-client#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "^3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.13.0"
  }
}
